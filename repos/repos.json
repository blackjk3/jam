{
  "author": "Jason Kadrmas <blackjk@gmail.com> (http://kadrmasconcepts.com/blog/) @itooamaneatguy",
  "name": "jaws",
  "description": "JavaScript Application Web Stack - Repositories",
  "version": "0.0.1",
  "repositories": {
    
    "backbone" : {
      "github": "documentcloud/backbone",
      "url": "https://raw.github.com/documentcloud/backbone/{version}/backbone.js",
      "min": "https://raw.github.com/documentcloud/backbone/{version}/backbone-min.js",
      "versioned": true 
    },
    "chosen" : {
      "github": "harvesthq/chosen",
      "url": "https://raw.github.com/harvesthq/chosen/{version}/chosen.jquery.js",
      "min": "https://raw.github.com/harvesthq/chosen/{version}/chosen.jquery.min.js",
      "versioned": true 
    },
    "jquery" : {
      "github": "jquery/jquery",
      "url": "http://code.jquery.com/jquery-{version}.js",
      "min": "http://code.jquery.com/jquery-{version}.min.js",
      "versioned": true 
    },
    "underscore" : {
      "github": "documentcloud/underscore",
      "url": "https://raw.github.com/documentcloud/underscore/{version}/underscore.js",
      "min": "https://raw.github.com/documentcloud/underscore/{version}/underscore-min.js",
      "versioned": true 
    }, 
    "ember" : {
      "github": "emberjs/ember.js",
      "url": "https://github.com/downloads/emberjs/ember.js/ember-{version}.js",
      "min": "https://github.com/downloads/emberjs/ember.js/ember-{version}.min.js",
      "versioned": true 
    },
    "zepto" : {
      "github": "madrobby/zepto",
      "url": "http://zeptojs.com/zepto.js",
      "min": "http://zeptojs.com/zepto.min.js",
      "versioned": false
    },
    "three.js" : {
      "github": "mrdoob/three.js",
      "url": "https://raw.github.com/mrdoob/three.js/{version}/build/Three.js",
      "min": "https://raw.github.com/mrdoob/three.js/{version}/build/Three.js",
      "versioned": false
    },
    "lodash" : {
      "github": "bestiejs/lodash",
      "url": "https://raw.github.com/bestiejs/lodash/{version}/lodash.js",
      "min": "https://raw.github.com/bestiejs/lodash/{version}/lodash.min.js",
      "versioned": true
    },
    "moment" : {
      "github": "timrwood/moment",
      "url": "https://raw.github.com/timrwood/moment/{version}/moment.js",
      "min": "https://raw.github.com/timrwood/moment/{version}/min/moment.min.js",
      "versioned": true
    },
    "select2" : {
      "github": "ivaynberg/select2",
      "url": "https://raw.github.com/ivaynberg/select2/{version}/select2.js",
      "min": "https://raw.github.com/ivaynberg/select2/{version}/select2.min.js",
      "versioned": true
    }
  }
}
